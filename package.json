{
    "name": "my-turborepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md,css,js,jsx}\""
    },
    "devDependencies": {
        "eslint-config-custom": "workspace:*",
        "prettier": "^3.0.3",
        "turbo": "^1.10.16"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "pnpm@7.26.0"
}
